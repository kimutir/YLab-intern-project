{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "target": "ES6",
    "module": "ES6",
    "jsx": "react-jsx",
    "noImplicitAny": false,
    "skipLibCheck": true,
    "allowJs": true,
    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {
      "@src/*": ["src/*"]
    },
    "allowSyntheticDefaultImports": true
  },
  "exclude": ["node_modules", "dist"]
}
